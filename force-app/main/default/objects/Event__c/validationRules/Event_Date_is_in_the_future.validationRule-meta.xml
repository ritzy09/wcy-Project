<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Event_Date_is_in_the_future</fullName>
    <active>true</active>
    <description>Event date should be in future</description>
    <errorConditionFormula>Event_Date__c  &lt; today()</errorConditionFormula>
    <errorMessage>you cannot enter a date from past</errorMessage>
</ValidationRule>
